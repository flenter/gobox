{
  "customSteps": {
  "setup_environment" : {
    "commands": [
      "export LANG=en_US.UTF-8",
      "export LC_ALL=en_US.UTF-8",
      "export GOPATH=$HOME/go"
      ]
    }
  },
  "preInstallScripts" : [
    "mkdir -p $HOME/go/src",
    "export GOPATH=$HOME/go",
    "yes | sudo apt-get -y install golang"
  ]
}
